{
    "name": "openapc_model",
    "label": "openapc standard model",
    "description": "model for open apc data",
    "cubes": [
        {
            "name": "openapc",
            "label": "openapc cube",
            "measures": [
                {
                    "name": "apc_amount",
                    "label": "APC Amount"
                },
                {
                    "name": "institution_distinct",
                    "label": "Institution"
                }
            ],
            "aggregates": [
                {
                    "name": "apc_amount_sum",
                    "label": "sum of apc",
                    "measure": "apc_amount",
                    "function": "sum"
                },
                {
                    "name": "institution",
                    "label": "distinct_institutions",
                    "measure": "institution_distinct",
                    "function": "count_distinct"
                }
            ],
            "dimensions": ["apc_amount"]
        }
    ],
    "mappings": [
        {
            "institution": "openapc.Institution",
            "apc_amount.apc_amount": "openapc.Euro"
        }
    ],
    "dimensions": [
        {
            "name": "apc_amount",
            "description": "The APC in Euro"
        },
        {
            "name": "institution_distinct",
            "description": "A data-providing institution (like unibi)"
        }
    ]
}
